<h1>New Restaurant</h1>

<%= simple_form_for(@restaurant) do |f| %>
  <%= f.input :name %>
  <%= f.input :address %>
  <%= f.input :category %>
  <%= f.input :phone_number %>
  <%= f.submit "Submit restaurant", class: "btn btn-primary" %>
<% end %>

<%= link_to "back to list", restaurants_path, class: "btn btn-primary m-2"%>
